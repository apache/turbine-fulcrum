<!-- This configuration file for Avalon components is used for testing the TestComponent -->
<role-list>
    <role
        name="org.apache.fulcrum.xmlrpc.XmlRpcClientComponent"
        shorthand="XmlRpcClientComponent"
        default-class="org.apache.fulcrum.xmlrpc.DefaultXmlRpcClientComponent"/>
    <role
        name="org.apache.fulcrum.xmlrpc.XmlRpcServerComponent"
        shorthand="XmlRpcServerComponent"
        default-class="org.apache.fulcrum.xmlrpc.DefaultXmlRpcServerComponent"/>
    <role
        name="org.apache.fulcrum.xmlrpc.TestHandler"
        shorthand="TestHandler"
        default-class="org.apache.fulcrum.xmlrpc.TestHandlerImpl"/>
</role-list>
