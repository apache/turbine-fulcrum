CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ID_TABLE(ID_TABLE_ID INTEGER NOT NULL PRIMARY KEY,TABLE_NAME VARCHAR(255),NEXT_ID INTEGER,QUANTITY INTEGER,CONSTRAINT SYS_CT_73 UNIQUE(TABLE_NAME))
CREATE MEMORY TABLE TURBINE_PERMISSION(PERMISSION_ID INTEGER NOT NULL PRIMARY KEY,PERMISSION_NAME VARCHAR(99) NOT NULL,OBJECTDATA BINARY,CONSTRAINT SYS_CT_75 UNIQUE(PERMISSION_NAME))
CREATE MEMORY TABLE TURBINE_ROLE(ROLE_ID INTEGER NOT NULL PRIMARY KEY,ROLE_NAME VARCHAR(99) NOT NULL,OBJECTDATA BINARY,CONSTRAINT SYS_CT_77 UNIQUE(ROLE_NAME))
CREATE MEMORY TABLE TURBINE_GROUP(GROUP_ID INTEGER NOT NULL PRIMARY KEY,GROUP_NAME VARCHAR(99) NOT NULL,OBJECTDATA BINARY,CONSTRAINT SYS_CT_79 UNIQUE(GROUP_NAME))
CREATE MEMORY TABLE TURBINE_ROLE_PERMISSION(ROLE_ID INTEGER NOT NULL,PERMISSION_ID INTEGER NOT NULL,PRIMARY KEY(ROLE_ID,PERMISSION_ID),CONSTRAINT SYS_FK_81 FOREIGN KEY(ROLE_ID) REFERENCES TURBINE_ROLE(ROLE_ID),CONSTRAINT SYS_FK_82 FOREIGN KEY(PERMISSION_ID) REFERENCES TURBINE_PERMISSION(PERMISSION_ID))
CREATE MEMORY TABLE TURBINE_USER(USER_ID INTEGER NOT NULL PRIMARY KEY,LOGIN_NAME VARCHAR(32) NOT NULL,PASSWORD_VALUE VARCHAR(32) NOT NULL,FIRST_NAME VARCHAR(99) NOT NULL,LAST_NAME VARCHAR(99) NOT NULL,EMAIL VARCHAR(99),CONFIRM_VALUE VARCHAR(99),MODIFIED TIMESTAMP,CREATED TIMESTAMP,LAST_LOGIN TIMESTAMP,OBJECTDATA BINARY,CONSTRAINT SYS_CT_87 UNIQUE(LOGIN_NAME))
CREATE MEMORY TABLE TURBINE_USER_GROUP_ROLE(USER_ID INTEGER NOT NULL,GROUP_ID INTEGER NOT NULL,ROLE_ID INTEGER NOT NULL,PRIMARY KEY(USER_ID,GROUP_ID,ROLE_ID),CONSTRAINT SYS_FK_89 FOREIGN KEY(USER_ID) REFERENCES TURBINE_USER(USER_ID),CONSTRAINT SYS_FK_90 FOREIGN KEY(GROUP_ID) REFERENCES TURBINE_GROUP(GROUP_ID),CONSTRAINT SYS_FK_91 FOREIGN KEY(ROLE_ID) REFERENCES TURBINE_ROLE(ROLE_ID))
CREATE MEMORY TABLE TURBINE_SCHEDULED_JOB(JOB_ID INTEGER NOT NULL PRIMARY KEY,SECOND INTEGER DEFAULT -1 NOT NULL,MINUTE INTEGER DEFAULT -1 NOT NULL,HOUR INTEGER DEFAULT -1 NOT NULL,WEEK_DAY INTEGER DEFAULT -1 NOT NULL,DAY_OF_MONTH INTEGER DEFAULT -1 NOT NULL,TASK VARCHAR(99) NOT NULL,EMAIL VARCHAR(99),PROPERTY BINARY)
